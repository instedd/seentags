%h4
  Edit Report Set #{@report_set.name}

= form_for :report_set, :url => { :controller => 'report_set', :action => :update } do |f|
  = f.error_messages
  %p
    = f.label :name
    %br/
    = f.text_field :name
  %p
    = f.label :url_callback, 'Callback'
    %br/
    = f.text_field :url_callback, :size => 75
  = submit_tag 'Update Report Set'

= link_to 'Go back', :controller => :home, :action => :home
